            $.post('/Info/GetCurrentCreditValue', function (data, status) {
                if (data.credit) {
                    if (data.credit != undefined) {
                        $('#creditA').html(arabicNum(data.credit.toLocaleString()) + "ريال");
                        $('#wallet-ballance').html(arabicNum(data.credit.toLocaleString()) + "ريال");
